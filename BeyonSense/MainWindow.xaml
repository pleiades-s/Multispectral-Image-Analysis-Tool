<Window x:Class="BeyonSense.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BeyonSense"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button x:Name="labeling" Content="Labeling" Height="200" Width="200" Margin="50"/>
            <Button x:Name="Train" Content="Train" Height="200" Width="200"/>
            <Button x:Name="Test" Content="Test" Height="200" Width="200" Margin="50"/>

        </StackPanel>
    </Grid>
</Window>
